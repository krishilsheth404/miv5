}

.memory {
  --text-primary: hsl(39, 100%, 56%);
  --bg-primary: hsla(39, 100%, 56%, 0.15);
}

.verbal {
  --text-primary: hsl(166, 100%, 37%);
  --bg-primary: hsla(166, 100%, 37%, 0.15);
